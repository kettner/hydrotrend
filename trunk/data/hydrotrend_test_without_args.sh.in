#!/bin/sh

mkdir -p @CMAKE_CURRENT_BINARY_DIR@/HYDRO_IN && \
mkdir -p @CMAKE_CURRENT_BINARY_DIR@/HYDRO_OUTPUT && \
cp -p @CMAKE_CURRENT_SOURCE_DIR@/input/HYDRO.IN \
      @CMAKE_CURRENT_BINARY_DIR@/HYDRO_IN && \
cp -p @CMAKE_CURRENT_SOURCE_DIR@/input/HYDRO0.HYPS \
      @CMAKE_CURRENT_BINARY_DIR@/HYDRO_IN && \
cd @CMAKE_CURRENT_BINARY_DIR@ && \
@HYDROTREND_EXE@ -V && \
diff @CMAKE_CURRENT_SOURCE_DIR@/output/HYDROASCII.Q HYDRO_OUTPUT/HYDROASCII.Q

